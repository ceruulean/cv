{
  "name": "gale-savage",
  "version": "1.0.0",
  "description": "website",
  "main": "index.js",
  "scripts": {
    "acc": "pa11y --reporter csv ./index.html > pa11y_report.csv",
    "serve": "http-server -c-1 -p 8081",
    "dist": "gulp --gulpfile gulpfile.js",
    "diststatic": "gulp --gulpfule gulpfile.js static scripts",
    "roll": "rollup -c --compact",
    "build": "npm run dist && npm run diststatic",
    "watch": "npm-watch run-src",
    "run-src-serve": "npm run dist && npm run serve",
    "run-src": "npm run dist"
  },
  "watch": {
    "run-src": {
      "patterns": [
        "src"
      ],
      "extensions": "html,js,css",
      "quiet": false
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "social-icon": "^1.2.1"
  },
  "devDependencies": {
    "@riverside/form-serialize": "^1.0.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat-css": "^3.1.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-run": "^1.7.1",
    "gulp-sourcemaps": "^2.6.5",
    "http-server": "^0.12.1",
    "micromodal": "^0.4.2",
    "npm-watch": "^0.6.0",
    "rollup": "^1.31.0",
    "rollup-plugin-terser": "^5.2.0"
  }
}
