{
  "name": "gale-savage",
  "version": "1.0.0",
  "description": "website",
  "main": "index.js",
  "scripts": {
    "acc": "pa11y --reporter csv ./index.html > pa11y_report.csv",
    "serve": "http-server -c-1 -p 8081",
    "dist": "gulp --gulpfile gulpfile.js",
    "roll": "rollup -c --compact",
    "test": "tape test/*.js",
    "watch": "npm-watch run-src",
    "run-src-serve": "npm run dist && npm run serve",
    "run-src": "npm run dist"
  },
  "watch": {
    "run-src": {
      "patterns": [
        "src"
      ],
      "extensions": "html,js,css",
      "quiet": false
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.1",
    "gulp-concat-css": "^3.1.0",
    "gulp-replace": "^1.0.0",
    "gulp-run": "^1.7.1",
    "http-server": "^0.12.1",
    "micromodal": "^0.4.2",
    "npm-watch": "^0.6.0",
    "rollup": "^1.31.0"
  }
}
